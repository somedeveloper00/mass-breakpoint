{
  "name": "mass-breakpoint",
  "displayName": "Mass Breakpoint",
  "description": "Set/remove breakpoints for all references results",
  "version": "0.0.1",
  "publisher": "Saeed Barari",
  "engines": { "vscode": "^1.60.0" },
  "activationEvents": [
    "onCommand:mass-breakpoint.setBreakpointsForReferences",
    "onCommand:mass-breakpoint.clearBreakpointsFromReferences"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "typescript": "^4.0.0",
    "vscode-test": "^1.6.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "mass-breakpoint.setBreakpointsForReferences",
        "title": "Mass Breakpoint: Set breakpoints for all References",
        "category": "Mass Breakpoint"
      },
      {
        "command": "mass-breakpoint.clearBreakpointsFromReferences",
        "title": "Mass Breakpoint: Clear breakpoints created from References",
        "category": "Mass Breakpoint"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "mass-breakpoint.setBreakpointsForReferences",
          "when": "editorLangId == javascript || editorLangId == typescript",
          "group": "navigation"
        },
        {
          "command": "mass-breakpoint.clearBreakpointsFromReferences",
          "when": "editorLangId == javascript || editorLangId == typescript",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "mass-breakpoint.setBreakpointsForReferences",
          "when": "editorLangId == javascript || editorLangId == typescript",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "mass-breakpoint.setBreakpointsForReferences",
          "when": "referenceSearchVisible",
          "group": "navigation"
        },
        {
          "command": "mass-breakpoint.clearBreakpointsFromReferences",
          "when": "referenceSearchVisible",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Mass Breakpoint",
      "properties": {
        "massBreakpoint.maxBreakpoints": {
          "type": "number",
          "default": 250,
          "description": "Maximum number of breakpoints that will be added by the mass breakpoints command."
        }
        ,
        "massBreakpoint.enableLogging": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable output logging to the 'Mass Breakpoint' output channel."
        }
      }
    }
  }
}
